{
    "name": "shah-newaz/redprint-ng",
    "description": "A Laravel package for generating CRUD operations with Vue.js frontend",
    "type": "library",
    "keywords": [
        "laravel",
        "api",
        "crud",
        "code generation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahnewaz Rifat",
            "email": "rifat_rifats@live.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shahnewaz\\RedprintNg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahnewaz\\RedprintNg\\RedprintServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Shahnewaz\\RedprintNg\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    }
}