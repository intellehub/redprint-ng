{
    "name": "shahnewaz/redprint-ng",
    "description": "Laravel CRUD Generator",
    "type": "library",
    "keywords": [
        "laravel",
        "api",
        "crud",
        "code generation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Shahnewaz Rifat",
            "email": "rifat_rifats@live.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "symfony/process": "^7.0",
        "friendsofphp/php-cs-fixer": "^3.6",
        "navindex/html-formatter": "^0.1.1",
        "mck89/peast": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Shahnewaz\\RedprintNg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahnewaz\\RedprintNg\\Providers\\RedprintServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Shahnewaz\\RedprintNg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
