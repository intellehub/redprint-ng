{
  "name": "shah-newaz/redprint-ng",
  "description": "Redprint Laravel App Builder",
  "keywords": ["laravel", "admin", "backend", "crud", "builder", "administrator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Shahnewaz Rifat",
      "email": "rifat_rifats@live.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "laravel/framework": "^8.0",
    "vlucas/phpdotenv": "^5.2",
    "shah-newaz/codecanyon-license-ng": "^1.0",
    "shah-newaz/permissible-ng": "^1.0",
    "intervention/image": "^2.5",
    "shah-newaz/redprint-unity-ng": "^1.0",
    "squizlabs/php_codesniffer": "3.*",
    "jackiedo/dotenv-editor": "1.*",
    "shah-newaz/laravel-config-writer": "^4.0.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
        "Shahnewaz\\RedprintNg\\": "src/"
    },
    "files": [
        "src/helpers/general.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
          "Shahnewaz\\RedprintNg\\Providers\\RedprintServiceProvider",
          "Shahnewaz\\RedprintNg\\Providers\\RedprintshipServiceProvider"
      ],
      "aliases": {
        "Redprint": "Shahnewaz\\RedprintNg\\Facades\\RedprintFacade",
        "Redprintship": "Shahnewaz\\RedprintNg\\Facades\\RedprintshipFacade"
      }
    }
  }
}
